// RUN: ./strip-comments.sh %s | filecheck %s --enable-var-scope

region_a:
    op 1
    op 1
    op 1

// CHECK-LABEL: region_a:
// CHECK-NEXT: op [[REG:\d+]]
// CHECK-NEXT: op [[REG]]
// CHECK-NEXT: op [[REG]]

region_b:
    op 2
    op 2
    op 1

// CHECK-LABEL: region_b:
// CHECK-NOT: op [[REG]]
// CHECK-NEXT: op [[REG:\d+]]
// CHECK-NEXT: op [[REG]]
// CHECK-NEXT: op 1
